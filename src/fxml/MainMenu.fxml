<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainMenuController">
   <children>
      <VBox alignment="CENTER" spacing="20" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Algorithm Quest">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </Label>
            <Button fx:id="bfsGameLevelsBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="BFS Game Levels" />
            <Button fx:id="bfsMultiplayerBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="BFS Multiplayer" />
            <Button fx:id="searchingLevelBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="Searching Algorithms" />
            <Button fx:id="sortingLevelBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="200.0" text="Sorting Algorithms" />
         </children>
      </VBox>
   </children>
</AnchorPane>